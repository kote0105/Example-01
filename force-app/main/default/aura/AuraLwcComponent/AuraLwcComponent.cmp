<aura:component implements="flexipage:availableForAllPageTypes">
    <aura:attribute name="message" type="string"/>
    <lightning:card title="Aura Lwc Communication">
        <div class="slds-p-around_medium">
            <div>{!v.message}</div>
        </div>
        <div class="slds-p-around_medium">
            <c:lwcAuraCommunication onsendMsg="{!c.msgHandler}" title="Hello this message from aura"></c:lwcAuraCommunication>
        </div>        
    </lightning:card>
</aura:component>	
